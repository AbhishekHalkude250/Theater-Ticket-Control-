{
  "CommonInfra": {
    "ElasticDB": {
      "version": 7,
      "configuration": {
        "nodes": ["http://localhost:9200"],
        "requestTimeout": 30000,
        "pingTimeout": 3000,
        "auth": {
          "username": "admin",
          "password": "admin"
        },
        "agent": {
          "maxSockets": 1,
          "keepAlive": false
        },
        "ssl": {
          "rejectUnauthorized": false
        }
      },
      "number_of_shards": 2,
      "number_of_replicas": 2
    }
  }
  
}
